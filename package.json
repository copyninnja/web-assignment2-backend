{
  "name": "movies-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/netlify-lambda build src clean compile:all",
    "clean": "rimraf ./build && mkdir build",
    "compile:moviemodels": "babel ./api/movies --out-dir build/api/movies",
    "compile:reviewModels": "babel ./api/reviews --out-dir build/api/reviews",
    "compile:ratingModels": "babel ./api/rating --out-dir build/api/rating",
    "compile:usermodels": "babel ./api/users --out-dir build/api/users",
    "compile:models": "npm-run-all compile:moviemodels compile:usermodels compile:reviewModels compile:ratingModels",
    "compile:seed": "babel ./seedData --out-dir build/seedData",
    "compile:routes": "babel ./routes --out-dir build/routes ",
    "compile:app": "babel index.js --out-dir build/",
    "compile:db": "babel db.js --out-dir build/",
    "compile:copy": "cp -r ./node_modules/ build/node_modules && cp -r ./public build/public",
    "compile:all": "npm-run-all compile:models compile:seed compile:app compile:db compile:copy",
    "postinstall": "rimraf ./build && npm run compile:all",
    "start": "./node_modules/.bin/netlify-lambda serve src",
    "test": "cross-env NODE_ENV=test mocha "
  },
  "author": "XiaoFeng Gong",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.0.2",
    "cross-env": "^6.0.3",
    "eslint": "^7.14.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "bcrypt-nodejs": "0.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.7.1",
    "moment": "^2.29.1",
    "mongoose": "^5.11.1",
    "morgan": "^1.10.0",
    "netlify-lambda": "^2.0.2",
    "node-fetch": "^2.6.1",
    "node-notifier": "^9.0.0",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "rimraf": "^3.0.0",
    "serverless-http": "^2.6.0",
    "xoauth2": "^1.2.0"
  }
}
